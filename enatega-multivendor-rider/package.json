{
  "name": "enatega-rider-app",
  "version": "5.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "eject": "expo eject",
    "postinstall": "patch-package",
    "format": "prettier --write '**/*.js'",
    "configure": "eas build:configure",
    "build:android": "eas build --platform android --profile production",
    "lint:fix": "eslint . --ext .js --fix",
    "build:staging": "eas build --profile staging -p all",
    "build:staging:android": "eas build --profile staging -p android",
    "build:staging:ios": "eas build --profile staging -p ios",
    "build:production": "eas build --profile production -p all",
    "build:production:android": "eas build --profile production -p android",
    "build:production:ios": "eas build --profile production -p ios",
    "submit:production": "eas submit --profile production -p all",
    "submit:production:android": "eas submit --profile production -p android",
    "submit:production:ios": "eas submit --profile production -p ios",
    "publish:staging": "ENVIRONMENT=staging expo publish --release-channel staging",
    "publish:production": "ENVIRONMENT=production expo publish --release-channel production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-firebase/app": "^21.11.0",
    "@react-native-firebase/messaging": "^21.11.0",
    "@react-navigation/bottom-tabs": "6.5.8",
    "@react-navigation/drawer": "^6.3.0",
    "@react-navigation/elements": "^1.3.22",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "6.0.11",
    "@sentry/react-native": "~6.14.0",
    "@types/react": "~19.0.10",
    "apollo-upload-client": "17.0.0",
    "deprecated-react-native-prop-types": "^4.0.0",
    "expo": "^53.0.0",
    "expo-application": "~6.1.5",
    "expo-av": "~15.1.7",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-keep-awake": "~14.1.4",
    "expo-localization": "~16.1.6",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-task-manager": "~13.1.6",
    "expo-updates": "~0.28.17",
    "graphql": "^15.8.0",
    "graphql-request": "^7.2.0",
    "i18n-js": "^3.7.1",
    "i18next": "^23.7.6",
    "lottie-react-native": "7.2.2",
    "metro": "^0.82.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "native-notify": "^4.0.9",
    "patch-package": "^6.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.79.5",
    "react-native-animatable": "^1.3.3",
    "react-native-background-fetch": "^4.2.8",
    "react-native-background-geolocation": "^4.18.9",
    "react-native-countdown-component": "^2.7.1",
    "react-native-flash-message": "^0.1.21",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-gifted-chat": "^2.6.4",
    "react-native-keyboard-controller": "^1.18.1",
    "react-native-loading-spinner-overlay": "^3.0.0",
    "react-native-maps": "1.20.1",
    "react-native-maps-directions": "^1.8.0",
    "react-native-material-textfield": "^0.16.1",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-system-setting": "^1.7.6",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "sentry-expo": "~7.0.0",
    "subscriptions-transport-ws": "^0.9.19",
    "toastify-react-native": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@expo/config-plugins": "~10.1.1",
    "@expo/metro-config": "^0.20.17",
    "@expo/prebuild-config": "~9.0.0",
    "babel-preset-expo": "~13.0.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "expo-gradle-ext-vars": "^0.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "2.0.5",
    "prettier-config-standard": "^1.0.1"
  },
  "private": true
}
